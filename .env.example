# Database
DATABASE_URL="postgresql://user:password@localhost:5432/db_name?schema=public"

# JWT
JWT_SECRET="your-secret-key"
JWT_EXPIRES_IN="7d"
JWT_REFRESH_SECRET="your-refresh-secret-key"
